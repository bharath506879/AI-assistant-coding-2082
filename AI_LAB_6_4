        "#Task 3: Banking Transaction Simulation"
      ],
      "metadata": {
        "id": "0vkxkYDz4ezO"
      }
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "cb0efd4e",
        "outputId": "cd66aa8e-c085-49ea-d6a4-237eedbcb255"
      },
      "source": [
        "class BankAccount:\n",
        "    def __init__(self, account_holder, balance):\n",
        "        self.account_holder = account_holder\n",
        "        self.balance = balance\n",
        "\n",
        "    def deposit(self, amount):\n",
        "        self.balance += amount\n",
        "        print(f\"Deposit successful. New balance for {self.account_holder}: ${self.balance}\")\n",
        "\n",
        "    def withdraw(self, amount):\n",
        "        if amount <= self.balance:\n",
        "            self.balance -= amount\n",
        "            print(f\"Withdrawal successful. New balance for {self.account_holder}: ${self.balance}\")\n",
        "        else:\n",
        "            print(\"Insufficient balance. Withdrawal not allowed.\")\n",
        "\n",
        "# Example usage\n",
        "account = BankAccount(\"bharath\", 5000)\n",
        "\n",
        "account.deposit(2000)\n",
        "account.withdraw(3000)\n",
        "account.withdraw(5000)"
      ],
      "execution_count": 2,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Deposit successful. New balance for bharath: $7000\n",
            "Withdrawal successful. New balance for bharath: $4000\n",
            "Insufficient balance. Withdrawal not allowed.\n"
          ]
        }
      ]
    },
