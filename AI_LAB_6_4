      "source": [
        "class Student:\n",
        "    def __init__(self, name, roll_number, marks):\n",
        "        self.name = name\n",
        "        self.roll_number = roll_number\n",
        "        self.marks = marks\n",
        "\n",
        "    def display_details(self):\n",
        "        print(f\"Name: {self.name}, Roll Number: {self.roll_number}, Marks: {self.marks}\")\n",
        "\n",
        "    def check_performance(self, class_average):\n",
        "        if self.marks > class_average:\n",
        "            return \"Student is performing above average\"\n",
        "        else:\n",
        "            return \"Student is performing below average\"\n",
        "\n",
        "# Example usage:\n",
        "# Create a student object\n",
        "student1 = Student(\"bhartah \", 101, 85)\n",
        "\n",
        "# Assume class average is 75\n",
        "avg_marks = 75\n",
        "\n",
        "# Display student details\n",
        "student1.display_details()\n",
        "\n",
        "# Print performance result\n",
        "print(student1.check_performance(avg_marks))"
      ]
    },
