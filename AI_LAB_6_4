        "# Sensor readings collected from a monitoring system\n",
        "sensor_readings = [12, 7, 5, 20, 9, 14, 3, 8]\n",
        "\n",
        "# Write a for loop to iterate over the sensor readings\n",
        "for reading in sensor_readings:\n",
        "    # Check if the reading is an even number using modulus operator\n",
        "    if reading % 2 == 0:\n",
        "        # Calculate the square of the reading\n",
        "        square = reading ** 2\n",
        "        # Print the result in the specified format\n",
        "        print(f\"Even reading: {reading}, Square: {square}\")"
      ],
