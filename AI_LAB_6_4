      "cell_type": "markdown",
      "source": [
        "#Task 5: Online Shopping Cart Module\n"
      ],
      "metadata": {
        "id": "DZOwafWq5VZe"
      }
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "40c0e7f9",
        "outputId": "bed6e3e9-b292-4040-a232-4c8134035ec7"
      },
      "source": [
        "class ShoppingCart:\n",
        "    def __init__(self):\n",
        "        self.items = []\n",
        "\n",
        "    def add_item(self, name, price, quantity):\n",
        "        item = {\"name\": name, \"price\": price, \"quantity\": quantity}\n",
        "        self.items.append(item)\n",
        "        print(f\"Added {quantity} x {name} to the cart.\")\n",
        "\n",
        "    def remove_item(self, name):\n",
        "        found = False\n",
        "        for item in self.items:\n",
        "            if item[\"name\"] == name:\n",
        "                self.items.remove(item)\n",
        "                found = True\n",
        "                print(f\"Removed {name} from the cart.\")\n",
        "                break\n",
        "        if not found:\n",
        "            print(f\"{name} not found in the cart.\")\n",
        "\n",
        "    def calculate_total(self):\n",
        "        total = 0\n",
        "        for item in self.items:\n",
        "            total += item[\"price\"] * item[\"quantity\"]\n",
        "\n",
        "        if total > 5000:\n",
        "            discount = total * 0.10\n",
        "            final_amount = total - discount\n",
        "            print(f\"Total before discount: ${total}. Applied 10% discount: ${discount:.2f}\")\n",
        "        else:\n",
        "            final_amount = total\n",
        "\n",
        "        return final_amount\n",
        "\n",
        "# Example usage\n",
        "cart = ShoppingCart()\n",
        "\n",
        "cart.add_item(\"Laptop\", 45000, 1)\n",
        "cart.add_item(\"Mouse\", 500, 2)\n",
        "cart.add_item(\"Keyboard\", 1500, 1)\n",
        "\n",
        "cart.remove_item(\"Mouse\")\n",
        "\n",
        "final_amount = cart.calculate_total()\n",
        "print(\"Final Amount to Pay:\", final_amount)"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Added 1 x Laptop to the cart.\n",
            "Added 2 x Mouse to the cart.\n",
            "Added 1 x Keyboard to the cart.\n",
            "Removed Mouse from the cart.\n",
            "Total before discount: $46500. Applied 10% discount: $4650.00\n",
            "Final Amount to Pay: 41850.0\n"
          ]
        }
      ]
    }
  ]
}