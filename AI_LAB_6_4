          "base_uri": "https://localhost:8080/"
        },
        "id": "aed51f55",
        "outputId": "b96e8a1d-9c43-4391-d388-505fdeffad04"
      },
      "source": [
        "# List of students with their scores\n",
        "students = [\n",
        "    {\"name\": \"bharath\", \"score\": 82},\n",
        "    {\"name\": \"Ananya\", \"score\": 74},\n",
        "    {\"name\": \"Kiran\", \"score\": 90},\n",
        "    {\"name\": \"Sneha\", \"score\": 68},\n",
        "    {\"name\": \"Vikram\", \"score\": 78}\n",
        "]\n",
        "\n",
        "# Initialize index for while loop\n",
        "index = 0\n",
        "\n",
        "# Use a while loop to iterate through the students list\n",
        "while index < len(students):\n",
        "    student = students[index]\n",
        "    # Check if student score is greater than 75\n",
        "    if student[\"score\"] > 75:\n",
        "        # Print the student's name in a readable format\n",
        "        print(f\"{student['name']} is eligible for scholarship with a score of {student['score']}.\")\n",
        "    # Increment the index properly to avoid infinite loop\n",
        "    index += 1"
      ],
      "execution_count": 4,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "bharath is eligible for scholarship with a score of 82.\n",
            "Kiran is eligible for scholarship with a score of 90.\n",
            "Vikram is eligible for scholarship with a score of 78.\n"
          ]
        }
      ]
